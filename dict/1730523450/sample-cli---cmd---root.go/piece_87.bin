package cmd

import (
	"fmt"

	"github.com/spf13/cobra"
)

func Execute() {
	var rootCmd = &cobra.Command{
		RunE: func(cmd *cobra.Command, args []string) error {
			return cmd.Usage()
		},
	}
	rootCmd.AddCommand(newTimeCmd())
	err := rootCmd.Execute()
	if err != nil {
		fmt.Printf("Failed to execute command: %s", err)
	}
}
